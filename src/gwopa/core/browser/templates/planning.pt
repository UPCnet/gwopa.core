<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="gwopa">
<body>

<metal:content-core fill-slot="main"
    tal:define="portal_state context/@@plone_portal_state;
                portal_url portal_state/portal_url;">
  <metal:block define-macro="content-core" tal:define="currentYear view/getYear">
    <div class="block-nav">
      <ul class="pagination pagination-lg" tal:define="items view/getItems">
        <tal:block tal:repeat="item items">
          <li tal:attributes="class item/classe; alt item/alt; title item/alt">
            <a href="#" tal:attributes="href item/url"><tal:omit-tag tal:content="item/title"></tal:omit-tag>&nbsp;<tal:omit-tag tal:content="item/year"></tal:omit-tag></a>
          </li>
        </tal:block>
      </ul>
    </div>
    <h1 class="documentFirstHeading">
      <tal:omit-tag i18n:translate=""> Planning </tal:omit-tag> -
      <span tal:content="view/projectTitle"></span></h1>
    <div class="documentDescription description projectDates">
      <span class="dateNumbers">
        <tal:omit-tag i18n:translate=""> Project year </tal:omit-tag>&nbsp;<tal:omit-tag tal:content="currentYear"> 2019 </tal:omit-tag>
      </span>
      <tal:omit-tag i18n:translate=""> goes from </tal:omit-tag>&nbsp;<span tal:content="view/getFaseStart" class="dateNumbers"> 2019-02-02 </span>&nbsp;<tal:omit-tag i18n:translate=""> to </tal:omit-tag>&nbsp;<span tal:content="view/getFaseEnd" class="dateNumbers"> 2019-03-03 </span>&nbsp;<span tal:content="view/getPhases" id="totalPhases" style="display: none"></span>
    </div>

    <ul class="nav nav-tabs" role="tablist">
      <li role="presentation" class="active"><a href="#project" aria-controls="project" role="tab" data-toggle="tab" i18n:translate=""> Project </a></li>
      <li role="presentation"><a href="#outcomes" aria-controls="outcomes" role="tab" data-toggle="tab" i18n:translate=""> Outcomes </a></li>
    </ul>

    <div class="tab-content">
      <div role="tabpanel" class="tab-pane active" id="project">
        <tal:block-activities tal:define="areas view/getAreas" tal:condition="areas">
          <div class="tabla_contenido_items container-fluid">
            <tal:objects repeat="item areas">
              <div tal:define="files python:view.indicatorsInside(item);" class="row-fluid">
                <div class="tabla_cabecera">
                  <div class="information" id="header">
                    <div class="contentt" id="total">
                      <div class="titleRowPlanning">
                        <i style="display: none" class="fa fa-chevron-down expandWaPla" aria-hidden="true"></i>
                        <i class="fa fa-chevron-up notexpandWaPla" aria-hidden="true"></i>
                        <div class="btn-group" role="group" aria-label="Add element">
                          <div data-toggle="dropdown"><i class="fas fa-plus-circle"></i></div>
                          <ul class="dropdown-menu" role="menu" aria-labelledby="types">
                            <li>
                              <a data-toggle="modal" class="afegirActivity" data-target="#modalActivity"
                                  tal:attributes="
                                      data-val string:${view/getPath}/${item/id};
                                      data-start view/project_start;
                                      data-end view/project_end">
                                <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> Add Activity </tal:omit-tag></a></li>
                          </ul>
                        </div>
                        <span tal:attributes="id string:${item/id}" class="expPlWA" tal:content="item/title"> Working Area Title </span>
                      </div>
                    </div>
                    <div class="contentt" id="bl" i18n:translate="">Starting date</div>
                    <div class="contentt" id="end" i18n:translate="">Completion date</div>
                    <div class="contentt" id="target" i18n:translate="">Target value</div>
                    <div class="contentt" id="resp" i18n:translate="">Responsible</div>
                    <div class="contentt" id="actions" i18n:translate=""><i class="far fa-edit"></i><i class="far fa-trash-alt"></i></div>
                  </div>
                </div>
                <div class="tabla_cuerpo" tal:repeat="item files">
                  <div class="col-md-12 text-item-planning">
                    <div class="information object">
                      <div class="contentt" id="total">
                        <div class="part0">
                          <tal:block tal:condition="python:item['portal_type'] == 'Activity'">
                            <div class="btn-group" role="group" aria-label="Add element">
                              <div data-toggle="dropdown"><i class="fas fa-plus-circle"></i></div>
                              <ul class="dropdown-menu" role="menu" aria-labelledby="types">
                                <li>
                                  <a data-toggle="modal" class="afegirOutput" data-target="#modalOutput"
                                      tal:attributes="
                                          data-val item/url;
                                          data-start item/start;
                                          data-end item/end">
                                    <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> Add Output </tal:omit-tag></a></li>
                              </ul>
                            </div>
                          </tal:block>
                        </div>
                        <div class="part1 label-2nd">
                          <span tal:attributes="class string:${item/portal_type}-planning first-level" tal:content="item/portal_type" i18n:translate="">Activity/output label</span>
                        </div>
                        <div class="part2 label-2nd">
                          <h5 class="titulocatplan" tal:attributes="id item/portal_type" tal:content="item/title">Activity</h5>
                        </div>
                      </div>
                      <div class="contentt" id="bl">
                        <span tal:content="item/start"> 2019-03-29 </span>
                      </div>
                      <div class="contentt" id="end">
                        <span tal:content="item/end"> 2019-03-30 </span>
                      </div>
                      <div class="contentt text_center" tal:condition="python:item['portal_type'] == 'Activity'" id="target">
                        <span tal:content="item/target_value_planned"> Target Value Planned </span>
                      </div>
                      <div class="contentt target" tal:condition="python:item['portal_type'] == 'Output'" id="target">
                        <span tal:attributes="
                            data-pk currentYear;
                            data-name item/url;
                            data-url string:${context/absolute_url}/changeTargetPlanning"
                            data-type="text"
                            data-title="Change target value"
                            class="editable" tal:content="item/target_value_planned"> Target Value Planned
                        </span>
                        <span tal:content="item/unit"> Unit </span>
                      </div>
                      <div class="contentt" id="resp" tal:define="members item/responsible">
                        <ul tal:condition="members"><li tal:repeat="member members" tal:content="member"> Member Name </li></ul>
                        <tal:block tal:condition="not:members" i18n:translate=""> - </tal:block>
                      </div>
                      <div class="contentt" id="actions">
                        <a class="pat-plone-modal"
                          tal:attributes="
                            href string:${item/url}/edit;
                            data-pat-plone-modal string: title: Edit ${item/title};
                            title string:Edit ${item/title};
                            alt string:Edit ${item/title}"><i class="far fa-edit"></i></a>
                        <a href="#confirm-delete" data-toggle="modal"  class="btn-delete"
                          tal:attributes="
                            data-url item/url;
                            data-type item/portal_type;
                            data-id item/title;" title="Delete" alt="Delete">
                          <i class="far fa-trash-alt"></i></a>
                      </div>
                    </div>
                    <tal:block tal:define="outputs python:view.indicatorsInside(item);">
                      <div tal:repeat="output outputs" class="block_sub_indicators">
                        <div class="information object">
                          <div class="contentt row" id="total">
                            <div class="part1 label-nopart0">
                              <span tal:attributes="class string:${output/portal_type}-planning second-level" tal:content="output/portal_type" i18n:translate="">Type Output</span>
                            </div>
                            <div class="part2 label-2nd">
                              <h5 class="titulocatplan" tal:attributes="id output/portal_type" tal:content="output/title"> Item Title </h5>
                            </div>
                          </div>
                          <div class="contentt" id="bl" tal:content="output/start">2019-03-29</div>
                          <div class="contentt" id="end" tal:content="output/end">2019-03-30</div>
                          <div class="contentt target" id="target">
                            <span tal:attributes="
                                data-pk currentYear;
                                data-name output/url;
                                data-url string:${context/absolute_url}/changeTargetPlanning"
                                data-type="text"
                                data-title="Change target value"
                                class="editable" tal:content="output/target_value_planned"> Target Value Planned
                            </span>
                            <span tal:content="output/unit"> Unit </span>
                          </div>
                          <div class="contentt" id="resp" tal:define="members output/responsible">
                            <ul tal:condition="members"><li tal:repeat="member members" tal:content="member"> Member Name </li></ul>
                            <ul tal:condition="not:members"><li i18n:translate=""> - </li></ul>
                          </div>
                          <div class="contentt" id="actions">
                            <a class="pat-plone-modal"
                              tal:attributes="
                                href string:${output/url}/edit;
                                data-pat-plone-modal string: title: Edit ${output/title};
                                title string:Edit ${output/title};
                                alt string:Edit ${output/title}"><i class="far fa-edit"></i></a>
                            <a href="#confirm-delete" data-toggle="modal"  class="btn-delete"
                              tal:attributes="
                                data-url output/url;
                                data-type output/portal_type;
                                data-id output/title;" title="Delete" alt="Delete">
                              <i class="far fa-trash-alt"></i></a>
                          </div>
                        </div>
                      </div>
                    </tal:block>
                  </div>
                </div>
              </div>
            </tal:objects>
          </div>
        </tal:block-activities>
      </div>
      <div role="tabpanel" class="tab-pane" id="outcomes">
        <div class="tabla_contenido_items">
          <div id="outcomesPerformance" class="row boxUtilityPerformance" tal:define="outcomes view/listOutcomesKPI">
            <div class="tabla_cabecera">
              <div class="information object" id="header">
                <div class="contentt" id="total">
                  <div class="titleRowPlanning" i18n:translate="">
                    <i style="display: none" class="fa fa-chevron-down expandPerPla" aria-hidden="true"></i>
                    <i class="fa fa-chevron-up notexpandPerPla" aria-hidden="true"></i>
                    <div class="btn-group" role="group" aria-label="Add KPI">
                      <div data-toggle="dropdown"><i class="fas fa-plus-circle"></i></div>
                      <ul class="dropdown-menu" role="menu" aria-labelledby="kpis">
                        <!-- <li>
                          <a data-toggle="modal" class="afegirKPI" data-target="#modalKPI"
                              tal:attributes="
                              data-val view/getPath;
                              data-start view/project_start;
                              data-end view/project_end">
                            <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> New Overall KPI </tal:omit-tag></a>
                        </li> -->
                        <li>
                          <a data-toggle="modal" class="afegirKPIZone" data-target="#modalKPIZone"
                              tal:attributes="
                                  data-val view/getPath;
                                  data-start view/project_start;
                                  data-end view/project_end">
                            <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> New KPI Zone </tal:omit-tag></a>
                        </li>
                      </ul>
                    </div>
                    <span id="utility">Performance </span>
                  </div>
                </div>
                <div class="contentt" id="bl" i18n:translate=""> Baseline date </div>
                <div class="contentt" id="end" i18n:translate=""> Baseline value </div>
                <div class="contentt" id="zone" i18n:translate=""> Zone </div>
                <div class="contentt" id="target" i18n:translate=""> Target value </div>
                <div class="contentt" id="resp" i18n:translate=""> Responsible </div>
                <div class="contentt" id="actions" i18n:translate=""><i class="far fa-edit"></i><i class="far fa-trash-alt"></i></div>
              </div>
            </div>
            <div class="tabla_cuerpo">
              <tal:objects repeat="outcome outcomes">
                <div class="col-md-12 text-item-planning">
                  <div class="information object">
                    <div class="contentt row" id="total">
                      <div class="part1 label-nopart0">
                        <!-- <tal:block condition="python:outcome['portal_type'] == 'OutcomeKPI'">
                          <span tal:attributes="class string:${outcome/portal_type}-planning second-level" i18n:translate="">Performance</span>
                        </tal:block> -->
                        <tal:block condition="python:outcome['portal_type'] == 'OutcomeZONE'">
                          <!-- <span tal:attributes="class string:${outcome/portal_type}-planning second-level" tal:content="outcome/portal_type" i18n:translate="">Outcome - KPI Type</span> -->
                          <span tal:attributes="class string:${outcome/portal_type}-planning second-level" i18n:translate="">Performance</span>
                        </tal:block>
                      </div>
                      <div class="part2 label-2nd">
                        <h5 class="titulocatplan" tal:content="outcome/title"> KPI </h5>
                      </div>
                    </div>
                    <div class="contentt" id="bl" tal:content="outcome/base_date">2019-03-30</div>
                    <div class="contentt" id="end" tal:content="outcome/base_value">2019-06-22</div>
                    <div class="contentt" id="zone" tal:content="outcome/zone"> Zone </div>
                    <div class="contentt target" id="target">
                      <span tal:attributes="
                          data-pk currentYear;
                          data-name outcome/url;
                          data-url string:${context/absolute_url}/changeTargetPlanning"
                          data-type="text"
                          data-title="Change target value"
                          class="editable" tal:content="outcome/target_value_planned"> Target Value Planned
                      </span>
                      <span tal:content="outcome/unit"> Unit </span>
                    </div>
                    <div class="contentt" id="resp" tal:define="members outcome/responsible">
                      <ul tal:condition="members"><li tal:repeat="member members" tal:content="member"> Member Name </li></ul>
                      <tal:block tal:condition="not:members" i18n:translate=""> - </tal:block>
                    </div>
                    <div class="contentt" id="actions">
                      <a class="pat-plone-modal"
                        tal:attributes="
                          href string:${outcome/url}/edit;
                          data-pat-plone-modal string: title: Edit ${outcome/title};
                          title string:Edit ${outcome/title};
                          alt string:Edit ${outcome/title}"><i class="far fa-edit"></i></a>
                      <a href="#confirm-delete" data-toggle="modal" class="btn-delete"
                        tal:attributes="
                              data-url outcome/url;
                              data-type outcome/portal_type;
                              data-id outcome/title;" title="Delete" alt="Delete">
                        <i class="far fa-trash-alt"></i></a>
                    </div>
                  </div>
                </div>
              </tal:objects>
            </div>
          </div>
          <div id="outcomesCapacity" class="row boxUtilityCapacity" tal:define="outcomes view/listOutcomesCC" tal:condition="outcomes">
            <div class="tabla_cabecera">
              <div class="information object" id="header">
                <div class="contentt" id="total">
                  <div class="titleRowPlanning" i18n:translate="">
                    <i style="display: none" class="fa fa-chevron-down expandCapPla" aria-hidden="true"></i>
                    <i class="fa fa-chevron-up notexpandCapPla" aria-hidden="true"></i>
                    <span id="capacity">Capacity </span>
                  </div>
                </div>
                <div class="contentt" id="actions" i18n:translate=""><i class="far fa-edit"></i></div>
              </div>
            </div>
            <div class="tabla_cuerpo">
              <div class="col-md-12 text-item-planning" tal:repeat="outcome outcomes">
                <div class="information object">
                  <div class="contentt row" id="total">
                    <div class="part0">
                      <i class="fa fa-chevron-down expandccs" aria-hidden="true"></i>
                      <i style="display: none" class="fa fa-chevron-up notexpandccs" aria-hidden="true"></i>
                    </div>
                    <div class="part1 label-2nd">
                      <span class="CC-planning" i18n:translate="">Capacity</span>
                    </div>
                    <div class="part2 label-2nd">
                      <h5 class="capacity-text">Improved capacity in/for </h5>
                      <h5 tal:attributes="id string:${outcome/rid};" class="capacity-title" tal:content="outcome/area"> Working Area N </h5>
                    </div>
                  </div>
                  <div class="contentt" id="actions">
                    <a data-toggle="modal" class="editOutcomeCC" data-target="#modalEditOutcomeCC"
                            tal:attributes="
                                data-pk currentYear;
                                data-urloutcomecc outcome/url;
                                data-description outcome/description;
                                data-base-value outcome/base_value;
                                data-base-date outcome/base_date;
                                data-objective outcome/objective;
                                data-objective-date outcome/objective_date;">
                        <i class="far fa-edit"></i></a>
                  </div>

                  <div class="information cc_container" >
                    <tal:block tal:repeat="item outcome/specifics">


                      <tal:block tal:condition="python: item['short_category'] != 'other'">
                        <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                        tal:attributes="
                                  data-pk currentYear;
                                  data-urloutcomeccs outcome/url;
                                  data-id_specific item/id_specific;
                                  data-description item/description;
                                  data-base-value item/baseline;
                                  data-base-date item/baseline_date;
                                  data-objective item/objective;
                                  data-objective-date item/objective_date;
                                  class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                          <div class="item_image" tal:condition="not:item/selected_specific">
                            <img tal:attributes="src string:${item/icon_url}" alt="" title=""/>
                          </div>
                          <div class="item_image " tal:condition="item/selected_specific">
                            <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                          </div>
                          <div class="item_text">
                            <span tal:content="item/title_specific">Leadership</span>
                          </div>
                        </a>
                      </tal:block>
                    </tal:block>
                  </div>
                  <div class="information cc_container_others">
                   <tal:block tal:repeat="item outcome/specifics">
                    <tal:block tal:condition="python: item['short_category'] == 'other'">
                      <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                        tal:attributes="
                                  data-pk currentYear;
                                  data-urloutcomeccs outcome/url;
                                  data-id_specific item/id_specific;
                                  data-description item/description;
                                  data-base-value item/baseline;
                                  data-base-date item/baseline_date;
                                  data-objective item/objective;
                                  data-objective-date item/objective_date;
                                  class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                        <div class="item_image" tal:condition="not:item/selected_specific">
                            <img tal:attributes="src string:${item/icon_url}" alt="" title=""/>
                        </div>
                        <div class="item_image " tal:condition="item/selected_specific">
                          <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                        </div>
                        <div class="item_text">
                          <span tal:content="item/title_specific">Other</span>
                        </div>
                      </a>
                    </tal:block>
                    </tal:block>
                    <a class="addOutcomeCCS item other" data-toggle="modal" data-target="#modalAddOutcomeCCS"
                     tal:attributes="
                                data-pk currentYear;
                                data-urloutcomeccs outcome/url;">
                      <div class="item_image">
                        <img src="++theme++gwopa.theme/assets/images/add.png" alt="" title=""/>
                      </div>
                      <div class="item_text">
                        <span>Add other</span>
                      </div>
                    </a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
     </div>
    </div>

    <div id="modalActivity" class="modal fade" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="modalAddActivity" data-dismiss="modal" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form id="toClearActivity">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="modalAddActivity" i18n:translate=""> Add Activity </h4>
            </div>
            <div class="modal-body" id="NewActivity">
              <div class="modal-url" id="ActivityPath" style="display:none"></div>
              <div class="row">
                <div class="form-group col-md-12" style="padding-bottom:15px;">
                  <label id="label-output" i18n:translate="">Title</label>
                  <span class="requiredfield" title="Required"> <i class="fas fa-circle"></i> </span>
                  <input class="form-control" id="act-title"/>
                </div>
                <div class="form-group col-md-12">
                    <label for="message-text" class="control-label" i18n:translate="">Description</label>
                    <textarea class="form-control" id="act-description"></textarea>
                </div>
                <div class="form-group col-md-12">
                    <label for="message-text" class="control-label" i18n:translate="">Initial situation description</label>
                    <textarea class="form-control" id="act-initialdescription"></textarea>
                </div>
                <div class="form-group col-md-12" style="margin-bottom:10px !important;">
                  <span class="legend">Remember that the related Project dates are: Start: <span class="modal-start"></span> End: <span class="modal-end"></span></span>
                </div>
                <div class="form-group row" style="padding-bottom:15px;">
                  <div class="col-xs-6 col-md-6">
                    <label class="control-label" i18n:translate="">Starting date</label>
                    <span class="requiredfield" title="Required"> <i class="fas fa-circle"></i> </span>
                    <input id="act-start" class="pat-pickadate" tal:attributes="data-pat-pickadate view/custom_pattern_options" />
                  </div>
                  <div class="col-xs-6 col-md-6">
                      <label class="control-label" i18n:translate=""> Completion date </label>
                      <span class="requiredfield" title="Required"> <i class="fas fa-circle"></i> </span>
                      <input id="act-end" class="pat-pickadate" tal:attributes="data-pat-pickadate view/custom_pattern_options" />
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-xs-6 col-md-6">
                    <label for="message-text" class="control-label" i18n:translate="">Assigned budget</label>
                    <div class="input-group">
                      <div class="input-group-addon" tal:content="view/project_currency"> $ </div>
                      <input type="text" id="act-budget" class="form-control">
                    </div>
                    <span tal:content="view/hidden_project_currency" style="display: none;" id="act-hidden-project-currency"></span>
                  </div>
                   <div class="col-xs-6 col-md-6">
                    <label for="message-text" class="control-label" i18n:translate="">Responsible people</label>
                    <input type="hidden" class="pat-select2" id="act-responsible"
                           i18n:attributes="placeholder search_users" data-pat-select2="vocabularyUrl:api-select2-users.json;"/>
                  </div>
                </div>
                <div class="form-group col-md-12">
                  <label for="message-text" class="control-label" i18n:translate="">Risks / Assumptions</label>
                  <textarea class="form-control" id="act-risks"></textarea>
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <a type="button" class="btn button-cancel" data-dismiss="modal" i18n:translate="">Cancel</a>
              <a type="submit" class="btn button-save" id="createActivityFromModal"  data-dismiss="modal" i18n:translate="">Create</a>
            </div>
          </form>
        </div>
      </div>
    </div>

    <div id="modalOutput" class="modal fade" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="modalAddOutput" data-dismiss="modal" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form id="toClearOutput">
            <div class="modal-header">
              <button type="button" class="close output" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="modalAddOutput" i18n:translate=""> Add Output </h4>
            </div>
            <div class="modal-body" id="NewOutput">
              <div class="modal-url" id="OutputPath" style="display:none;"></div>
              <div class="row">
                <div class="form-group col-md-12">
                  <label id="label-output" i18n:translate="">Title</label>
                  <span class="requiredfield" title="Required"> <i class="fas fa-circle"></i> </span>
                  <input id="out-title" type="hidden"/>
