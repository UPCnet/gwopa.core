<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="gwopa">
<body>

<metal:content-core fill-slot="main"
    tal:define="portal_state context/@@plone_portal_state;
                portal_url portal_state/portal_url;
                gwopa_utils context/@@gwopa.utils;">

  <tal:omit-tag-ini tal:define="canViewPlanningMonitoring gwopa_utils/canViewPlanningMonitoring">
  <metal:block tal:condition="not: canViewPlanningMonitoring">
   <h1 class="documentFirstHeading" i18n:translate="">Insufficient permissions</h1>
  </metal:block>

  <metal:block tal:condition="canViewPlanningMonitoring" define-macro="content-core"
    tal:define="currentYear view/getYear;
                canEditPlanningMonitoring gwopa_utils/canEditPlanningMonitoring">
    <div class="block-nav">
      <ul class="pagination pagination-lg" tal:define="items view/getItems">
        <tal:block tal:repeat="item items">
          <li tal:attributes="class item/classe; alt item/alt; title item/alt; data-project-year item/year;">
            <a href="#" tal:attributes="href item/url"><tal:omit-tag tal:content="item/title"></tal:omit-tag>&nbsp;<tal:omit-tag tal:content="item/year"></tal:omit-tag></a>
          </li>
        </tal:block>
      </ul>
    </div>
    <h1 class="documentFirstHeading">
      <tal:omit-tag i18n:translate=""> Planning </tal:omit-tag> -
      <span tal:content="view/projectTitle"></span></h1>
    <div class="documentDescription description projectDates">
      <span class="dateNumbers">
        <tal:omit-tag i18n:translate=""> Project year </tal:omit-tag>&nbsp;<tal:omit-tag tal:content="currentYear"> 2019 </tal:omit-tag>
      </span>
      <tal:omit-tag i18n:translate=""> runs from </tal:omit-tag>&nbsp;<span tal:content="view/getFaseStart" class="dateNumbers"> 2019-02-02 </span>&nbsp;<tal:omit-tag i18n:translate=""> to </tal:omit-tag>&nbsp;<span tal:content="view/getFaseEnd" class="dateNumbers"> 2019-03-03 </span>&nbsp;<span tal:content="view/getPhases" id="totalPhases" style="display: none"></span>
    </div>

    <ul class="nav nav-tabs" role="tablist" id="myTab">
      <li role="presentation" class="active"><a href="#project" aria-controls="project" role="tab" data-toggle="tab" i18n:translate=""> Project </a></li>
      <li role="presentation"><a href="#outcomes" aria-controls="outcomes" role="tab" data-toggle="tab" i18n:translate=""> Outcomes </a></li>
    </ul>

    <div class="tab-content">
      <div role="tabpanel" class="tab-pane active" id="project">
        <tal:block-activities tal:define="areas view/getAreas" tal:condition="areas">
          <div class="tabla_contenido_items container-fluid">
            <tal:objects repeat="area areas">
              <div tal:define="activities python:view.activitiesInside(area);" class="row-fluid">
                <div class="tabla_cabecera">
                  <div class="information" id="header">
                    <div class="contentt" id="total">
                      <div class="titleRowPlanning">
                        <tal:block condition="activities">
                          <i style="display: none" class="fa fa-chevron-down expandWaPla" aria-hidden="true"></i>
                          <i class="fa fa-chevron-up notexpandWaPla" aria-hidden="true"></i>
                        </tal:block>
                        <tal:block condition="not:activities">
                          <span style="margin-left: 15px;">&nbsp;</span>
                        </tal:block>
                        <div class="btn-group" role="group" aria-label="Add element">
                          <div tal:condition="canEditPlanningMonitoring" data-toggle="dropdown"><i class="fas fa-plus-circle"></i></div>
                          <ul class="dropdown-menu" role="menu" aria-labelledby="types">
                            <li>
                              <a data-toggle="modal" class="afegirActivity" data-target="#modalActivity"
                                  tal:attributes="
                                      data-val string:${view/getPath}/${area/id};
                                      data-start view/project_start;
                                      data-end view/project_end;">
                                <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> Add Activity </tal:omit-tag></a></li>
                          </ul>
                        </div>
                        <span tal:attributes="id string:${area/id}" class="expPlWA" tal:content="area/title"> Working Area Title </span>
                      </div>
                    </div>
                    <tal:block condition="activities">
                      <div class="contentt" id="bl" i18n:translate="">Starting date</div>
                      <div class="contentt" id="end" i18n:translate="">Completion date</div>
                      <div class="contentt" id="target" i18n:translate="">Target value</div>
                      <div class="contentt" id="resp" i18n:translate="">Responsible</div>
                    </tal:block>
                  </div>
                </div>
                <div class="tabla_cuerpo" tal:repeat="activity activities">
                  <div class="col-md-12 text-item-planning">
                    <div class="information object">
                      <div class="contentt" id="total">
                        <div class="part0">
                          <div class="btn-group" role="group" aria-label="Add element">
                            <div tal:condition="canEditPlanningMonitoring" data-toggle="dropdown"><i class="fas fa-plus-circle"></i></div>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="types">
                              <li>
                                <a data-toggle="modal" class="afegirOutput" data-target="#modalOutput"
                                    tal:attributes="
                                        data-val activity/url;
                                        data-start activity/start;
                                        data-end activity/end">
                                  <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> Add Output </tal:omit-tag></a></li>
                            </ul>
                          </div>
                        </div>
                        <div class="part1 label-2nd">
                          <span tal:attributes="class string:${activity/portal_type}-planning first-level" tal:content="activity/portal_type" i18n:translate="">Activity/output label</span>
                        </div>
                        <div class="part2 label-2nd">
                          <h5 class="titulocatplan" tal:attributes="id activity/portal_type" tal:content="activity/title">Activity</h5>
                        </div>
                      </div>
                      <div class="contentt" id="bl">
                        <span tal:content="activity/start"> 2019-03-29 </span>
                      </div>
                      <div class="contentt" id="end">
                        <span tal:content="activity/end"> 2019-03-30 </span>
                      </div>
                      <div class="contentt text_center" id="target">
                        <span tal:content="activity/target_value_planned"> Target Value Planned </span>
                      </div>
                      <div class="contentt" id="resp" tal:define="members activity/responsible">
                        <ul tal:condition="members"><li tal:repeat="member members" tal:content="member"> Member Name </li></ul>
                        <tal:block tal:condition="not:members" i18n:translate=""> - </tal:block>
                      </div>
                      <div tal:condition="canEditPlanningMonitoring" class="contentt" id="actions">
                        <a class="pat-plone-modal"
                          tal:attributes="
                            href string:${activity/url}/edit;
                            data-pat-plone-modal string: title: Edit ${activity/title};
                            title string:Edit ${activity/title};
                            alt string:Edit ${activity/title}"><i class="far fa-edit"></i></a>
                        <a href="#confirm-delete" data-toggle="modal"  class="btn-delete"
                          tal:attributes="
                            data-url activity/url;
                            data-type activity/portal_type;
                            data-id activity/title;" i18n:attributes="title Delete; alt Delete">
                          <i class="far fa-trash-alt"></i></a>
                      </div>
                    </div>
                    <!-- Ouputs inside activity -->
                    <tal:block tal:define="outputs python:view.outputsInside(activity);">
                      <div tal:repeat="output outputs" class="block_sub_indicators">
                        <div class="information object">
                          <div class="contentt row" id="total">
                            <div class="part1 label-nopart0">
                              <span tal:attributes="class string:${output/portal_type}-planning second-level" tal:content="output/portal_type" i18n:translate="">Type Output</span>
                            </div>
                            <div class="part2 label-2nd">
                              <h5 class="titulocatplan" tal:attributes="id output/portal_type" tal:content="output/title"> Item Title </h5>
                            </div>
                          </div>
                          <div class="contentt" id="bl" tal:content="output/start">2019-03-29</div>
                          <div class="contentt" id="end" tal:content="output/end">2019-03-30</div>
                          <div class="contentt target" id="target">
                            <span tal:attributes="
                                data-pk currentYear;
                                data-name output/url;
                                data-url string:${context/absolute_url}/changeTargetPlanning"
                                data-type="text"
                                i18n:attributes="data-title Change_target_value"
                                class="editable" tal:content="output/target_value_planned"> Target Value Planned
                            </span>
                            <span tal:content="output/unit_text"> Unit </span>
                          </div>
                          <div class="contentt" id="resp" tal:define="members output/responsible">
                            <ul tal:condition="members"><li tal:repeat="member members" tal:content="member"> Member Name </li></ul>
                            <ul tal:condition="not:members"><li i18n:translate=""> - </li></ul>
                          </div>
                          <div tal:condition="canEditPlanningMonitoring" class="contentt" id="actions">
                        <!--     <a class="pat-plone-modal"
                              tal:attributes="
                                href string:${output/url}/edit;
                                data-pat-plone-modal string: title: Edit ${output/title};
                                title string:Edit ${output/title};
                                alt string:Edit ${output/title}"><i class="far fa-edit"></i></a> -->

                            <a data-toggle="modal" class="editOutput" data-target="#modalEditOutput"
                            tal:attributes="
                                data-pk currentYear;
                                data-urloutput output/url;
                                data-title output/title;
                                data-description output/description;
                                data-start activity/start;
                                data-end activity/end;
                                data-limit-start output/limit_start;
                                data-limit-end output/limit_end;
                                data-target-value-planned output/target_value_planned;
                                data-unit output/unit;
                                data-unit-text output/unit_text;
                                data-responsible output/responsible;
                                data-responsible-id output/responsible_id;
                                data-means output/means;
                                ">
                                <!-- data-risks output/risks; -->
                              <i class="far fa-edit"></i></a>
                            <a href="#confirm-delete" data-toggle="modal"  class="btn-delete"
                              tal:attributes="
                                data-url output/url;
                                data-type output/portal_type;
                                data-id output/title;" i18n:attributes="title Delete; alt Delete">
                              <i class="far fa-trash-alt"></i></a>
                          </div>
                        </div>
                      </div>
                    </tal:block>
                  </div>
                </div>
              </div>
            </tal:objects>
          </div>
        </tal:block-activities>
      </div>
      <div role="tabpanel" class="tab-pane" id="outcomes">
        <div class="tabla_contenido_items">
          <div id="outcomesPerformance" class="row boxUtilityPerformance" tal:define="outcomes view/listOutcomesKPI">
            <div class="tabla_cabecera">
              <div class="information object" id="header">
                <div class="contentt" id="total">
                  <div class="titleRowPlanning">
                    <tal:block condition="outcomes">
                      <i style="display: none" class="fa fa-chevron-down expandPerPla" aria-hidden="true"></i>
                      <i class="fa fa-chevron-up notexpandPerPla" aria-hidden="true"></i>
                    </tal:block>
                    <div class="btn-group" role="group" aria-label="Add KPI">
                      <div tal:condition="canEditPlanningMonitoring" data-toggle="dropdown"><i class="fas fa-plus-circle"></i></div>
                      <ul class="dropdown-menu" role="menu" aria-labelledby="kpis">
                        <li>
                          <a data-toggle="modal" class="afegirKPIZone" data-target="#modalKPIZone"
                              tal:attributes="
                                  data-val view/getPath;
                                  data-start view/project_start;
                                  data-end view/project_end;
                                  data-frequency view/project_frequency">
                            <i class="fas fa-plus-circle"></i><tal:omit-tag i18n:translate=""> Add New Overall KPI </tal:omit-tag></a>
                        </li>
                      </ul>
                    </div>
                    <span id="utility"  i18n:translate="">Performance </span>
                  </div>
                </div>
                <tal:block condition="outcomes">
                  <div class="contentt" id="zone" i18n:translate=""> Zone </div>
                  <div class="contentt" id="bl" i18n:translate=""> Baseline date </div>
                  <div class="contentt" id="end" i18n:translate=""> Baseline value </div>
                  <div class="contentt" id="target" i18n:translate=""> Target value </div>
                  <div class="contentt" id="resp" i18n:translate=""> Responsible </div>
                </tal:block>
              </div>
            </div>
            <div class="tabla_cuerpo">
              <tal:objects repeat="outcome outcomes">
                <div class="col-md-12">
                  <div class="information object">
                    <div class="contentt row" id="total">
                      <div class="part1 label-nopart0">
                        <tal:block condition="python:outcome['portal_type'] == 'OutcomeZONE'">
                          <span tal:attributes="class string:${outcome/portal_type}-planning second-level" i18n:translate="">Performance</span>
                        </tal:block>
                      </div>
                      <div class="part2 label-2nd">
                        <h5 class="titulocatplan" tal:content="outcome/title"> KPI </h5>
                      </div>
                    </div>
                    <div class="contentt" id="zone" tal:content="outcome/zone"> Zone </div>
                    <div class="contentt" id="bl" tal:content="outcome/base_date">2019-03-30</div>
                    <div class="contentt" id="end" tal:content="outcome/base_value">2019-06-22</div>
                    <div class="contentt target" id="target">
                      <span tal:attributes="
                          data-pk currentYear;
                          data-name outcome/url;
                          data-url string:${context/absolute_url}/changeTargetPlanning"
                          data-type="text"
                          i18n:attributes="data-title Change_target_value"
                          class="editable" tal:content="outcome/target_value_planned"> Target Value Planned
                      </span>
                      <span tal:content="outcome/unit_text"> Unit </span>
                    </div>
                    <div class="contentt" id="resp" tal:define="members outcome/responsible">
                      <ul tal:condition="members"><li tal:repeat="member members" tal:content="member"> Member Name </li></ul>
                      <tal:block tal:condition="not:members" i18n:translate=""> - </tal:block>
                    </div>
                    <div tal:condition="canEditPlanningMonitoring" class="contentt" id="actions">
       <!--                <a class="pat-plone-modal"
                        tal:attributes="
                          href string:${outcome/url}/edit;
                          data-pat-plone-modal string: title: Edit ${outcome/title};
                          title string:Edit ${outcome/title};
                          alt string:Edit ${outcome/title}"><i class="far fa-edit"></i></a> -->

                      <a data-toggle="modal" class="editKPIZone" data-target="#modalEditKPIZone"
                            tal:attributes="
                                    data-pk currentYear;
                                    data-urlkpi outcome/url;
                                    data-title outcome/title;
                                    data-zone outcome/zone;
                                    data-responsible outcome/responsible;
                                    data-responsible-id outcome/responsible_id;
                                    data-description outcome/description;
                                    data-base-value outcome/base_value;
                                    data-base-date outcome/base_date_edit;
                                    data-unit outcome/unit;
                                    data-unit-text outcome/unit_text;
                                    data-target-value-planned outcome/target_value_planned;
                                    data-means outcome/means;
                                    data-start view/project_start;
                                    data-end view/project_end;"><i class="far fa-edit"></i></a>
                                    <!-- data-risks outcome/risks; -->

                      <a href="#confirm-delete" data-toggle="modal" class="btn-delete"
                        tal:attributes="
                              data-url outcome/url;
                              data-type outcome/portal_type;
                              data-id outcome/title;" i18n:attributes="title Delete; alt Delete">
                        <i class="far fa-trash-alt"></i></a>
                    </div>
                  </div>
                </div>
              </tal:objects>
            </div>
          </div>
          <div id="outcomesCapacity" class="row boxUtilityCapacity" tal:define="outcomes view/listOutcomesCC" tal:condition="outcomes">
            <div class="tabla_cabecera">
              <div class="information object" id="header">
                <div class="contentt" id="total">
                  <div class="titleRowPlanning">
                    <tal:block condition="outcomes">
                      <i style="display: none" class="fa fa-chevron-down expandCapPla" aria-hidden="true"></i>
                      <i class="fa fa-chevron-up notexpandCapPla" aria-hidden="true"></i>
                    </tal:block>
                    <span id="capacity" i18n:translate="">Capacity </span>
                  </div>
                </div>
              </div>
            </div>
            <div class="tabla_cuerpo">
              <div class="col-md-12" tal:repeat="outcome outcomes">
                <div class="information object" tal:attributes="id string:${outcome/rid};">
                  <div class="contentt row" id="total">
                    <div class="part0">
                      <i class="fa fa-chevron-down expandccs" aria-hidden="true"></i>
                      <i style="display: none" class="fa fa-chevron-up notexpandccs" aria-hidden="true"></i>
                    </div>
                    <div class="part1 label-2nd">
                      <span class="CC-planning" i18n:translate="">Capacity</span>
                    </div>
                    <div class="part2 label-2nd">
                      <h5 class="capacity-text" i18n:translate="">Improved capacity in/for </h5>
                      <h5 tal:attributes="id string:${outcome/rid};" class="capacity-title" tal:content="outcome/area"> Working Area N </h5>
                    </div>
                  </div>
                  <div class="contentt" id="actions">
                    <a tal:condition="canEditPlanningMonitoring" data-toggle="modal" class="editOutcomeCC" data-target="#modalEditOutcomeCC"
                            tal:attributes="
                                data-pk currentYear;
                                data-urloutcomecc outcome/url;
                                data-description outcome/description;
                                data-base-value outcome/base_value;
                                data-base-date outcome/base_date;
                                data-objective outcome/objective;
                                data-objective-date outcome/objective_date;
                                data-id-capacity outcome/rid;">
                        <i class="far fa-edit"></i></a>
                      <a data-toggle="modal" data-target="#modalInfoCapacity" class="btn-info-capacity">
                        <i class="fa fa-info-circle"></i></a>
                  </div>
                  <div class="information cc_container" >
                    <div class="primerafila">
                      <tal:block repeat="item outcome/specifics">
                        <tal:block condition="python: item['short_category'] == 'transformational'"
                                   tal:define="titleSpecific python:view.getTitleSpecific(item)">
                          <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                          tal:attributes="
                                    data-pk currentYear;
                                    data-urloutcomeccs outcome/url;
                                    data-id_specific item/id_specific;
                                    data-title_specific titleSpecific;
                                    data-base-value item/baseline;
                                    data-base-date item/baseline_date;
                                    data-objective item/objective;
                                    data-objective-date item/objective_date;
                                    data-id-capacity outcome/rid;
                                    class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                            <div class="item_image" tal:condition="not:item/selected_specific">
                              <img tal:attributes="src item/icon_url;
                                                   data-selected item/icon_url_selected;"
                                   alt="" title=""/>
                            </div>
                            <div class="item_image " tal:condition="item/selected_specific">
                              <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                            </div>
                            <div class="item_text">
                              <span tal:content="titleSpecific">Leadership</span>
                            </div>
                          </a>
                        </tal:block>
                      </tal:block>
                    </div>
                    <div class="segundafila">
                      <tal:block repeat="item outcome/specifics">
                        <tal:block condition="python: item['short_category'] == 'transactional'"
                                   tal:define="titleSpecific python:view.getTitleSpecific(item)">
                          <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                          tal:attributes="
                                    data-pk currentYear;
                                    data-urloutcomeccs outcome/url;
                                    data-id_specific item/id_specific;
                                    data-title_specific titleSpecific;
                                    data-base-value item/baseline;
                                    data-base-date item/baseline_date;
                                    data-objective item/objective;
                                    data-objective-date item/objective_date;
                                    data-id-capacity outcome/rid;
                                    class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                            <div class="item_image" tal:condition="not:item/selected_specific">
                              <img tal:attributes="src item/icon_url;
                                                   data-selected item/icon_url_selected;"
                                   alt="" title=""/>
                            </div>
                            <div class="item_image " tal:condition="item/selected_specific">
                              <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                            </div>
                            <div class="item_text">
                              <span tal:content="titleSpecific">Leadership</span>
                            </div>
                          </a>
                        </tal:block>
                      </tal:block>
                    </div>
                    <div class="tercerafila">
                      <tal:block repeat="item outcome/specifics">
                        <tal:block condition="python: item['short_category'] == 'individual'"
                                   tal:define="titleSpecific python:view.getTitleSpecific(item)">
                          <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                          tal:attributes="
                                    data-pk currentYear;
                                    data-urloutcomeccs outcome/url;
                                    data-id_specific item/id_specific;
                                    data-title_specific titleSpecific;
                                    data-base-value item/baseline;
                                    data-base-date item/baseline_date;
                                    data-objective item/objective;
                                    data-objective-date item/objective_date;
                                    data-id-capacity outcome/rid;
                                    class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                            <div class="item_image" tal:condition="not:item/selected_specific">
                              <img tal:attributes="src item/icon_url;
                                                   data-selected item/icon_url_selected;"
                                   alt="" title=""/>
                            </div>
                            <div class="item_image " tal:condition="item/selected_specific">
                              <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                            </div>
                            <div class="item_text">
                              <span tal:content="titleSpecific">Leadership</span>
                            </div>
                          </a>
                        </tal:block>
                      </tal:block>
                    </div>
                  </div>
                  <div class="information cc_container_others">
                    <div class="cuartafila">
                      <tal:block repeat="item outcome/specifics">
                        <tal:block condition="python: item['short_category'] == 'other'"
                                   tal:define="titleSpecific python:view.getTitleSpecific(item)">
                          <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                            tal:attributes="
                                    data-pk currentYear;
                                    data-urloutcomeccs outcome/url;
                                    data-id_specific item/id_specific;
                                    data-title_specific titleSpecific;
                                    data-base-value item/baseline;
                                    data-base-date item/baseline_date;
                                    data-objective item/objective;
                                    data-objective-date item/objective_date;
                                    data-id-capacity outcome/rid;
                                    class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                            <div class="item_image" tal:condition="not:item/selected_specific">
                              <img tal:attributes="src item/icon_url;
                                                   data-selected item/icon_url_selected;"
                                   alt="" title=""/>
                            </div>
                            <div class="item_image " tal:condition="item/selected_specific">
                              <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                            </div>
                            <div class="item_text">
                              <span tal:content="titleSpecific">Other</span>
                            </div>
                          </a>
                        </tal:block>
                      </tal:block>
                      <a tal:condition="canEditPlanningMonitoring" class="addOutcomeCCS item other" data-toggle="modal" data-target="#modalAddOutcomeCCS" tal:attributes=" data-pk currentYear; data-urloutcomeccs outcome/url;">
                        <div class="item_image">
                          <img src="++theme++gwopa.theme/assets/images/add.png" alt="" title=""/>
                        </div>
                        <div class="item_text">
                          <span i18n:translate="">Add other</span>
                        </div>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div id="modalActivity" class="modal fade" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="modalAddActivity" data-dismiss="modal" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form id="toClearActivity">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
