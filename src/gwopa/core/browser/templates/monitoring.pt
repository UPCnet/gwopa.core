    <div id="modalEditOutcomeCCS" class="modal fade" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="modalEditOutcomeCCS" data-dismiss="modal" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form id="toClearEditOutcomeCC">
             <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="modalEditOutcomeCCS" i18n:translate=""> Edit Specific </h4>
            </div>
            <div class="modal-body" id="EditOutcomeCCS" >
              <div class="modal-url" id="OutcomeCCSPath" style="display:none"></div>
              <span class="modal-pk" id="OutcomeCCSYear" style="display:none"></span>
              <span class="modal-id" id="idSpecific" style="display:none"></span>
              <span class="modal-idCapacity" id="idCapacity" style="display:none"></span>
              <div class="row">
                <div class="form-group col-md-12">
                    <label for="message-text" class="control-label" i18n:translate="">Description</label>
                    <textarea readonly class="form-control" id="outcomeccs-description"></textarea>
                </div>
                <div class="form-group row">
                      <div class="col-xs-6 col-md-6">
                    <label for="message-text" class="control-label" i18n:translate="">Baseline value</label>
                    <input readonly class="form-control" id="outcomeccs-baseline" />
                  </div>
                  <div class="col-xs-6 col-md-6">
                      <label class="control-label" i18n:translate="">Baseline date</label>
                      <input readonly class="form-control" id="outcomeccs-baseline_date" />
                  </div>
                </div>
                <div class="form-group row">
                      <div class="col-xs-6 col-md-6">
                    <label for="message-text" class="control-label" i18n:translate="">Objective</label>
                    <input readonly class="form-control" id="outcomeccs-objective" />
                  </div>
                  <div class="col-xs-6 col-md-6">
                      <label class="control-label" i18n:translate="">Objective date</label>
                      <input readonly class="form-control" id="outcomeccs-objective_date"/>
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-xs-4 col-md-4">
                      <label i18n:translate=""> Degree of change </label>
                      <span class="requiredfield" title="Required"> <i class="fas fa-circle"></i> </span>
                      <input id="outcomeccs-degree_changes" style="width: 100%;" type="hidden"/>
                  </div>
                  <div class="col-xs-4 col-md-4">
                      <label i18n:translate=""> How much has contributed </label>
                      <input id="outcomeccs-contributed_project" style="width: 100%;" type="hidden"/>
                  </div>
                  <div class="col-xs-4 col-md-4">
                      <label i18n:translate=""> Consensus </label>
                      <input id="outcomeccs-consensus" style="width: 100%;" type="hidden"/>
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-xs-6 col-md-6">
                      <label i18n:translate=""> Main Obstacles </label>
                      <input id="outcomeccs-obstacles" style="width: 100%;" type="hidden"/>
                  </div>
                  <div class="col-xs-6 col-md-6">
                      <label i18n:translate=""> Main Contributing factors </label>
                      <input id="outcomeccs-contributing_factors" style="width: 100%;" type="hidden"/>
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-xs-12 col-md-12">
                    <label class="control-label" i18n:translate="">Explain</label>
                    <input class="form-control" id="outcomeccs-explain" />
                  </div>
                </div>
              </div>
            </div>
            <div class="modal-footer">
              <a type="button" class="btn button-cancel" data-dismiss="modal" i18n:translate="">Cancel</a>
              <a type="submit" class="btn button-save" id="updateOutcomeCCSFromModal"  data-dismiss="modal" i18n:translate="">Update</a>
            </div>
          </form>
        </div>
      </div>
