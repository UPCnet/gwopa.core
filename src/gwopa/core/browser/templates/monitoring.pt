<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="gwopa">
<body>

<metal:content-core fill-slot="main"
    tal:define="portal_state context/@@plone_portal_state;
                portal_url portal_state/portal_url;">
  <metal:block define-macro="content-core" tal:define="currentYear view/getYear">
    <div class="block-nav">
      <ul class="pagination pagination-lg" tal:define="items view/getItems">
        <tal:block tal:repeat="item items">
          <li tal:attributes="class item/classe; alt item/alt; title item/alt">
            <a href="#" tal:attributes="href item/url"><tal:omit-tag tal:content="item/title"></tal:omit-tag>&nbsp;<tal:omit-tag tal:content="item/year"></tal:omit-tag></a>
          </li>
        </tal:block>
      </ul>
    </div>
    <h1 class="documentFirstHeading">
      <tal:omit-tag i18n:translate=""> Monitoring </tal:omit-tag> -
      <span tal:content="view/projectTitle"></span></h1>
    <div class="documentDescription description projectDates">
      <span class="dateNumbers">
        <tal:omit-tag i18n:translate=""> Project year </tal:omit-tag>&nbsp;<tal:omit-tag tal:content="currentYear"> 2019 </tal:omit-tag>
      </span>
      <tal:omit-tag i18n:translate=""> goes from </tal:omit-tag>&nbsp;<span tal:content="view/getFaseStart" class="dateNumbers"> 2019-02-02 </span>&nbsp;<tal:omit-tag i18n:translate=""> to </tal:omit-tag>&nbsp;<span tal:content="view/getFaseEnd" class="dateNumbers"> 2019-03-03 </span>&nbsp;<span tal:content="view/getPhases" id="totalPhases" style="display: none"></span>
    </div>

    <ul class="nav nav-tabs" role="tablist" id="myTab">
      <li role="presentation" class="active"><a href="#project" aria-controls="project" role="tab" data-toggle="tab" i18n:translate=""> Project </a></li>
      <li role="presentation"><a href="#outcomes" aria-controls="outcomes" role="tab" data-toggle="tab" i18n:translate=""> Outcomes </a></li>
    </ul>

    <div class="tab-content">
      <div role="tabpanel" class="tab-pane active" id="project">
        <div id="activityWorkingArea" class="row tabla_contenido_items" tal:define="areas view/getAreas">
          <tal:objects repeat="area areas">
            <tal:block tal:define="files python:view.indicatorsInside(area);">
              <div class="tabla_cabecera">
                <div class="information object" id="header">
                  <div class="contentt" id="total">
                    <div class="titleRowMonitoring">
                      <tal:block condition="files">
                        <i style="display: none" class="fa fa-chevron-down expandWaMo" aria-hidden="true"></i>
                        <i class="fa fa-chevron-up notexpandWaMo" aria-hidden="true"></i>
                      </tal:block>
                      <tal:block condition="not:files">
                        <span style="margin-left: 15px;">&nbsp;</span>
                      </tal:block>
                      <span tal:attributes="id string:${area/id}" tal:content="area/title"> Working Area Title </span>
                    </div>
                  </div>
                  <tal:block condition="files">
                    <div class="contentt" id="bl" i18n:translate=""> Starting date </div>
                    <div class="contentt" id="comple" i18n:translate="">Completion date </div>
                    <div class="contentt" id="pyv" i18n:translate=""> Achieved/Target </div>
                    <div class="contentt" id="nexupd" i18n:translate=""> Next update </div>
                  </tal:block>
                </div>
              </div>
              <div class="tabla_cuerpo" tal:repeat="item files">
                <div class="col-md-12 text-item-monitoring">
                  <div class="information object">
                    <div class="contentt" id="total">
                      <div class="part0">
                        <i class="fa fa-chevron-down expandActMo" aria-hidden="true"></i>
                        <i style="display: none" class="fa fa-chevron-up notexpandActMo" aria-hidden="true"></i>
                        <tal:block tal:condition="item/updated">
                          <span class="dot">
                            <i id="done" class="fas fa-circle"></i>
                          </span>
                        </tal:block>
                        <tal:block tal:condition="not: item/updated">
                          <span class="dot">
                            <i id="todo" class="fas fa-circle"></i>
                          </span>
                        </tal:block>
                      </div>
                      <div class="part1">
                        <span tal:attributes="class string:${item/portal_type}-planning" tal:content="item/portal_type" i18n:translate=""> Activity/output label</span>
                      </div>
                      <div class="part2 label-2nd">
                        <span class="titulocatplan" tal:attributes="id string:${item/id}" tal:content="item/title"> Activity/Output Title </span>
                      </div>
                    </div>
                    <div class="contentt" id="bl" tal:content="item/start">2019-03-29</div>
                    <div class="contentt" id="comple" tal:content="item/end">28-02-2020</div>
                    <div class="contentt" id="pyv">
                      <tal:block tal:condition="python:item['portal_type'] == 'Output'">
                        <span tal:content="item/target_value_real"> 200 </span> / <span tal:content="item/target_value_planned"> 200 </span>
                        <span tal:content="item/unit"> m3 </span>
                      </tal:block>
                      <tal:block tal:condition="python:item['portal_type'] == 'Activity'">  -----  </tal:block>
                    </div>
                    <div class="contentt" id="nexupd" tal:content="item/next_update"> 18-12-2022</div>
                  </div>
                  <tal:comment replace="nothing"> <!-- Activity desplegable --> </tal:comment>
                  <div class="tabla_cuerpo monitoring_info" tal:attributes="id string:${item/id}-expandA">
                    <form id="updateActivity">
                      <div class="col-md-12 text-item-planning expandedBox" id="#" role="tabpanel" aria-expanded="true">
                        <div class="filacontenido">
                          <div class="col-sm-2">
                            <h5 i18n:translate="">Degree (%)</h5>
                            <input type="text" tal:attributes="id string:${item/id}-progress; value item/progress;">
                          </div>
                          <div class="col-sm-4">
                            <h5 i18n:translate="">Explanation Progress</h5>
                            <textarea class="form-control" rows="1" tal:content="item/explanation" tal:attributes="id string:${item/id}-explanation;"></textarea>
                          </div>
                          <div class="col-sm-3">
                            <h5 i18n:translate="">Main Obstacles</h5>
                            <input type="hidden" tal:attributes="id string:${item/id}-activity-obs; value item/obstacles;" style="width: 100%" placeholder="Search..."/>
                            <div tal:attributes="id string:${item/id}-addObstacleActivity">
                              <i class="fas fa-plus-circle" aria-hidden="true"></i>&nbsp;
                              <span class="legend"> Add new Obstacle if not listed</span>
                            </div>
                            <div tal:attributes="id string:${item/id}-addObstaclesActivity" class="form-group col-sm-12">
                              <h5 i18n:translate=""> New Main Obstacle </h5>
                              <input type="text" tal:attributes="id string:${item/id}-add-obs" placeholder="Type Obstacle..."/>
                            </div>
                          </div>
                          <div class="col-sm-3">
                            <h5 i18n:translate="">Main Contributing factors</h5>
                            <input type="hidden" tal:attributes="id string:${item/id}-activity-contrib; value item/contributing;" style="width: 100%" placeholder="Search..." />
                            <div tal:attributes="id string:${item/id}-addContributingActivity">
                              <i class="fas fa-plus-circle" aria-hidden="true"></i>&nbsp;
                              <span class="legend"> Add new Contributing factor </span>
                            </div>
                            <div tal:attributes="id string:${item/id}-addContributingsActivity" class="form-group col-sm-12">
                              <h5 i18n:translate=""> New Main Contributing factor </h5>
                              <input type="text" tal:attributes="id string:${item/id}-add-contrib" placeholder="Type Contributing factor..."/>
                            </div>
                          </div>
                        </div>
                        <div class="filacontenido">
                          <div class="col-sm-6">
                            <h5 i18n:translate="">Consideration for further planning</h5>
                            <textarea class="form-control" rows="1" tal:content="item/consideration" tal:attributes="id string:${item/id}-consideration;"></textarea>
                          </div>
                          <div class="col-sm-6">
                            <h5 i18n:translate="">Explain contributed and limiting factors</h5>
                            <textarea class="form-control" rows="1" tal:content="item/limiting" tal:attributes="id string:${item/id}-limiting;"></textarea>
                          </div>
                        </div>
                        <div tal:content="item/year" id="year" style="display:none;"></div>
                        <div tal:content="item/path" tal:attributes="id string:${item/id}-path" style="display:none;"></div>
                        <button type="submit" tal:attributes="id string:${item/id}-updateActivityMonitoring; class string:${item/id} btn btn-default focus"> Update </button>
                      </div>
                    </form>
                  </div>
                  <tal:block tal:define="outputs python:view.outputsInside(item);">
                    <div tal:repeat="output outputs" class="block_sub_indicators">
                      <div class="information object">
                        <div class="contentt" id="total">
                          <div class="part0">
                            <i class="fa fa-chevron-down expandOutMo" aria-hidden="true"></i>
                            <i style="display: none" class="fa fa-chevron-up notexpandOutMo" aria-hidden="true"></i>
                            <tal:block tal:condition="output/updated">
                              <span class="dot">
                                <i id="done" class="fas fa-circle"></i>
                              </span>
                            </tal:block>
                            <tal:block tal:condition="not: output/updated">
                              <span class="dot">
                                <i id="todo" class="fas fa-circle"></i>
                              </span>
                            </tal:block>
                          </div>
                          <div class="part1">
                            <span tal:attributes="class string:${output/portal_type}-planning second-level" tal:content="output/portal_type" i18n:translate="">Type Output</span>
                          </div>
                          <div class="part2 label-2nd">
                            <span class="titulocatplan" tal:attributes="id output/id" tal:content="output/title">Item Title</span>
                          </div>
                        </div>
                        <div class="contentt" id="bl" tal:content="output/start">2019-03-29</div>
                        <div class="contentt" id="value" tal:content="output/end">2019-03-30</div>
                        <div class="contentt target" id="pyv">
                          <tal:exclamation tal:condition="python:output['target_value_real'] != output['target_value_planned']">
                            <i class="fas fa-exclamation" title="Target value differs from planned"></i>
                          </tal:exclamation>
                          <span tal:attributes="
                              data-pk currentYear;
                              data-name output/url;
                              data-url string:${context/absolute_url}/changeTargetMonitoring"
                              data-type="text"
                              data-title="Change target value"
                              class="editable" tal:content="output/target_value_real"> Target Value Real
                          </span> /
                          <span tal:content="output/target_value_planned"> Target Value Planned </span>
                          <span tal:content="output/unit"> Unit </span>
                        </div>
                        <div class="contentt" id="nexupd" tal:content="output/next_update"> 18-12-2022</div>
                      </div>
                    <div class="tabla_cuerpo monitoring_info" tal:attributes="id string:${output/id}-expandO">
                      <div class="col-md-12 text-item-planning expandedBox" id="#" role="tabpanel" aria-expanded="true">
                        <form id="updateOutput">
                          <div class="filacontenido">
                            <div class="col-sm-1">
                              <h5 i18n:translate=""> Value </h5>
                                <input type="text" tal:attributes="id string:${output/id}-progress; value output/progress;">
                            </div>
                            <div class="col-sm-5">
                              <h5 i18n:translate="">Explanation Progress</h5>
                              <textarea class="form-control" rows="1" tal:content="output/explanation" tal:attributes="id string:${output/id}-explanation;"></textarea>
                            </div>
                            <div class="col-sm-3">
                              <h5 i18n:translate="">Main Obstacles</h5>
                              <input type="hidden" tal:attributes="id string:${output/id}-output-obs; value output/obstacles;" style="width: 100%"/>
                              <div tal:attributes="id string:${output/id}-addObstacleOutput" class="form-group col-sm-12" style="padding: 0px;">
                                <i class="fas fa-plus-circle" aria-hidden="true"></i>&nbsp;
                                <span class="legend">Add new Obstacle if not listed</span>
                              </div>
                              <div tal:attributes="id string:${output/id}-addObstaclesOutput" class="form-group col-sm-12">
                                <h5 i18n:translate="">Main Obstacles title</h5>
                                <input type="text" tal:attributes="id string:${output/id}-add-obs" placeholder="Type title..."/>
                              </div>
                            </div>
                            <div class="col-sm-3">
                              <h5 i18n:translate="">Main Contributing factors</h5>
                              <input type="hidden" tal:attributes="id string:${output/id}-output-contrib; value output/contributing;" style="width: 100%"/>
                              <div tal:attributes="id string:${output/id}-addContributingOutput" class="form-group col-sm-12" style="padding: 0px;">
                                <i class="fas fa-plus-circle" aria-hidden="true"></i>&nbsp;
                                <span class="legend">Add new Contributing factor</span>
                              </div>
                              <div tal:attributes="id string:${output/id}-addContributingsOutput" class="form-group col-sm-12">
                                <h5 i18n:translate="">Main Contributing factors title</h5>
                                <input type="text" tal:attributes="id string:${output/id}-add-contrib" placeholder="Type title..."/>
                              </div>
                            </div>
                          </div>
                          <div class="filacontenido">
                            <div class="col-sm-6">
                              <h5 i18n:translate="">Consideration for further planning</h5>
                              <textarea class="form-control" rows="1" tal:content="output/consideration" tal:attributes="id string:${output/id}-consideration;"></textarea>
                            </div>
                            <div class="col-sm-6">
                              <h5 i18n:translate="">Explain contributed and limiting factors</h5>
                              <textarea class="form-control" rows="1" tal:content="output/limiting" tal:attributes="id string:${output/id}-limiting;"></textarea>
                            </div>
                          </div>
                          <div tal:content="output/year" id="year" style="display:none;"></div>
                          <div tal:content="output/path" tal:attributes="id string:${output/id}-path" style="display:none;"></div>
                          <button type="submit" tal:attributes="id string:${output/id}-updateOutputMonitoring; class string:${output/id} btn btn-default focus"> Update </button>
                        </form>
                      </div>
                    </div>
                    </div>
                  </tal:block>
                </div>
              </div>
            </tal:block>
          </tal:objects>
        </div>
      </div>
      <div role="tabpanel" class="tab-pane" id="outcomes">
        <div class="tabla_contenido_items">
          <div id="outcomesPerformance" class="row boxUtilityPerformance" tal:define="outcomes view/listOutcomesKPI">
            <div class="tabla_cabecera">
              <div class="information object" id="header">
                <div class="contentt" id="total">
                  <div class="titleRowMonitoring">
                    <tal:block condition="outcomes">
                      <i style="display: none" class="fa fa-chevron-down expandPerMo" aria-hidden="true"></i>
                      <i class="fa fa-chevron-up notexpandPerMo" aria-hidden="true"></i>
                    </tal:block>
                    <span id="utility" i18n:translate=""> Utility Performance </span>
                  </div>
                </div>
                <tal:block condition="outcomes">
                  <div class="contentt" id="zone" i18n:translate="">Zone</div>
                  <div class="contentt" id="comple" i18n:translate="">Baseline date</div>
                  <div class="contentt" id="pyv" i18n:translate="">Achieved/Target</div>
                  <div class="contentt" id="nexupd" i18n:translate="">Next update</div>
                </tal:block>
              </div>
            </div>
            <div class="tabla_cuerpo" tal:repeat="outcome outcomes">
              <div class="col-md-12">
                <div class="information object">
                  <div class="contentt" id="total2">
                    <div class="part0">
                      <i class="fa fa-chevron-down expandKPIMo" aria-hidden="true"></i>
                      <i style="display: none" class="fa fa-chevron-up notexpandKPIMo" aria-hidden="true"></i>
                      <span class="dot" tal:condition="outcome/updated">
                        <i id="done" class="fas fa-circle"></i>
                      </span>
                      <span class="dot" tal:condition="not: outcome/updated">
                        <i id="todo" class="fas fa-circle"></i>
                      </span>
                    </div>
                    <div class="part11">
                      <tal:block condition="python:outcome['portal_type'] == 'OutcomeZONE'">
                        <span tal:attributes="class string:${outcome/portal_type}-planning second-level" i18n:translate="">Performance</span>
                      </tal:block>
                    </div>
                    <div class="part2">
                      <h5 class="titulocatplan" tal:attributes="id string:${outcome/id}" tal:content="outcome/title"> KPI </h5>
                    </div>
                  </div>
                  <div class="contentt" id="zone" tal:content="outcome/zone"> Zone </div>
                  <div class="contentt" id="comple" tal:content="outcome/base_date">2019-03-30</div>
                  <div class="contentt target" id="pyv">
                    <tal:exclamation tal:condition="python:outcome['target_value_real'] != outcome['target_value_planned']">
                      <i class="fas fa-exclamation"></i>
                    </tal:exclamation>
                    <span tal:attributes="
                        data-pk currentYear;
                        data-name outcome/url;
                        data-url string:${context/absolute_url}/changeTargetMonitoring"
                        data-type="text"
                        data-title="Change target value"
                        class="editable" tal:content="outcome/target_value_real"> Target Value Real
                    </span> /
                    <span tal:content="outcome/target_value_planned"> Target Value Planned </span>
                    <span tal:content="outcome/unit"> Unit</span>
                  </div>
                  <div class="contentt" id="nexupd" tal:content="outcome/next_update"> 18-12-2022</div>
                </div>
                <tal:comment replace="nothing">  <!-- KPI desplegable --> </tal:comment>
                <div class="tabla_cuerpo monitoring_info" tal:attributes="id string:${outcome/id}-expandK">
                  <form id="updateKPI">
                    <div class="col-md-12 text-item-planning expandedBox" id="#" role="tabpanel" aria-expanded="true">
                      <div class="filacontenido">
                        <div class="col-sm-1">
                          <h5 i18n:translate="">Current</h5>
                          <input type="text" tal:attributes="id string:${outcome/id}-progress; value outcome/progress;">
                        </div>
                        <div class="col-sm-5">
                          <h5 i18n:translate="">Explanation Progress</h5>
                          <textarea class="form-control" rows="1" tal:content="outcome/explanation" tal:attributes="id string:${outcome/id}-explanation;"></textarea>
                        </div>
                        <div class="col-sm-3">
                          <h5 i18n:translate="">Main Obstacles</h5>
                          <input type="hidden" tal:attributes="id string:${outcome/id}-kpi-obs; value outcome/obstacles;" style="width: 100%"/>
                          <div tal:attributes="id string:${outcome/id}-addObstacleKPI" class="form-group col-sm-12" style="padding: 0px;">
                            <i class="fas fa-plus-circle" aria-hidden="true"></i>&nbsp;
                            <span class="legend">Add new Obstacle if not listed</span>
                          </div>
                          <div tal:attributes="id string:${outcome/id}-addObstaclesKPI" class="form-group col-sm-12">
                            <h5 i18n:translate="">Main Obstacles title</h5>
                            <input type="text" tal:attributes="id string:${outcome/id}-add-obs" placeholder="Type title..."/>
                          </div>
                        </div>
                        <div class="col-sm-3">
                          <h5 i18n:translate="">Main Contributing factors</h5>
                          <input type="hidden" tal:attributes="id string:${outcome/id}-kpi-contrib; value outcome/contributing;" style="width: 100%"/>
                          <div tal:attributes="id string:${outcome/id}-addContributingKPI" class="form-group col-sm-12" style="padding: 0px;">
                            <i class="fas fa-plus-circle" aria-hidden="true"></i>&nbsp;
                            <span class="legend">Add new Contributing factor</span>
                          </div>
                          <div tal:attributes="id string:${outcome/id}-addContributingsKPI" class="form-group col-sm-12">
                            <h5 i18n:translate="">Main Contributing factors title</h5>
                            <input type="text" tal:attributes="id string:${outcome/id}-add-contrib" placeholder="Type title..."/>
                          </div>
                        </div>
                      </div>
                      <div class="filacontenido">
                        <div class="col-sm-6">
                          <h5 i18n:translate="">Consideration for further planning</h5>
                          <textarea class="form-control" rows="1" tal:content="outcome/consideration" tal:attributes="id string:${outcome/id}-consideration;"></textarea>
                        </div>
                        <div class="col-sm-6">
                          <h5 i18n:translate="">Explain contributed and limiting factors</h5>
                          <textarea class="form-control" rows="1" tal:content="outcome/limiting" tal:attributes="id string:${outcome/id}-limiting;"></textarea>
                        </div>
                      </div>
                      <div tal:content="outcome/year" id="year" style="display:none;"></div>
                      <div tal:content="outcome/path" tal:attributes="id string:${outcome/id}-path" style="display:none;"></div>
                      <button type="submit" tal:attributes="id string:${outcome/id}-updateKPIMonitoring; class string:${outcome/id} btn btn-default focus"> Update </button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
          <div id="outcomesCapacity" class="row boxUtilityCapacity" tal:define="outcomes view/listOutcomesCC" tal:condition="outcomes">
            <div class="tabla_cabecera">
              <div class="information object" id="header">
                <div class="contentt" id="total">
                  <div class="titleRowMonitoring">
                    <tal:block condition="outcomes">
                      <i style="display: none" class="fa fa-chevron-down expandCapMo" aria-hidden="true"></i>
                      <i class="fa fa-chevron-up notexpandCapMo" aria-hidden="true"></i>
                    </tal:block>
                    <span id="capacity" i18n:translate="">Capacity </span>
                  </div>
                </div>
              </div>
            </div>
              <div class="col-md-12" style="padding:9px 0px 1px 15px;" tal:repeat="outcome outcomes">
                <div class="information object" tal:attributes="id string:${outcome/rid};">
                  <div class="contentt row">
                    <div class="part0">
                      <i class="fa fa-chevron-down expandccs" aria-hidden="true"></i>
                      <i style="display: none" class="fa fa-chevron-up notexpandccs" aria-hidden="true"></i>
                    </div>
                    <div class="part1 label-2nd">
                      <span class="CC-planning" i18n:translate="">Capacity</span>
                    </div>
                    <div class="part2 label-2nd">
                      <h5 class="capacity-text">Improved capacity in/for </h5>
                      <h5 tal:attributes="id string:${outcome/rid};" class="capacity-title" tal:content="outcome/area"> Working Area N </h5>
                    </div>
                  </div>
                  <div class="information cc_container" >
                    <div class="primerafila">
                      <tal:block repeat="item outcome/monitoring">
                        <tal:block condition="python: item['short_category'] == 'transformational'">
                          <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                          tal:attributes="
                                    data-pk currentYear;
                                    data-urloutcomeccs outcome/url;
                                    data-id_specific item/id_specific;
                                    data-description item/description;
                                    data-base-value item/baseline;
                                    data-base-date item/baseline_date;
                                    data-objective item/objective;
                                    data-objective-date item/objective_date;
                                    data-degree-changes item/degree_changes;
                                    data-contributed-project item/contributed_project;
                                    data-contributing-factors item/contributing_factors;
                                    data-obstacles item/obstacles;
                                    data-explain item/explain;
                                    data-consensus item/consensus;
                                    data-selected-monitoring item/selected_monitoring;
                                    data-id-capacity outcome/rid;
                                    class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                            <tal:block tal:condition="python: item['selected_monitoring'] != 'notset'">
                              <i tal:attributes="class string:fas fa-circle ${item/selected_monitoring}; id string:${item/selected_monitoring};"></i>
                            </tal:block>
                            <tal:block tal:condition="python: item['selected_monitoring'] == 'notset'">
                              <i class="fas fa-circle" style="display: none"></i>
                            </tal:block>
                            <div class="item_image" tal:condition="not:item/selected_specific">
                              <img tal:attributes="src item/icon_url;
                                                   data-selected item/icon_url_selected;"
                                   alt="" title=""/>
                            </div>
