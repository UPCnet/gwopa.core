<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="gwopa">
<body>

<metal:content-core fill-slot="main"
    tal:define="portal_state context/@@plone_portal_state;
                portal_url portal_state/portal_url;">
  <metal:block define-macro="content-core" tal:define="currentYear view/getYear">
    <div class="block-nav">
      <ul id="selectYear" class="pagination pagination-lg" tal:define="items view/getItems">
        <tal:block tal:repeat="item items">
          <li tal:attributes="class string: ${item/classe}; alt item/alt; title item/alt; id item/year">
            <a href="#">
              <tal:omit-tag tal:content="item/title">
              </tal:omit-tag>&nbsp;<tal:omit-tag tal:content="item/year"></tal:omit-tag>
            </a>
          </li>
        </tal:block>
      </ul>
    </div>
    <h1 class="documentFirstHeading">
      <tal:omit-tag i18n:translate=""> Visualizating </tal:omit-tag> -
      <span tal:content="view/projectTitle"></span></h1>
    <div class="documentDescription description projectDates">
      <span class="dateNumbers">
        <tal:omit-tag i18n:translate=""> Project year </tal:omit-tag>&nbsp;<tal:omit-tag tal:content="currentYear"> 2019 </tal:omit-tag>
      </span>
      <tal:omit-tag i18n:translate=""> goes from </tal:omit-tag>&nbsp;<span tal:content="view/getFaseStart" class="dateNumbers"> 2019-02-02 </span>&nbsp;<tal:omit-tag i18n:translate=""> to </tal:omit-tag>&nbsp;<span tal:content="view/getFaseEnd" class="dateNumbers"> 2019-03-03 </span>&nbsp;<span tal:content="view/getPhases" id="totalPhases" style="display: none"></span>
    </div>
    <div tal:define="areas view/getAreas">
      <select id="selectWA">
        <option tal:repeat="area areas" tal:attributes="value area/url" tal:content="area/title">WA1</option>
      </select>
    </div>
    <div class="charts">
      <div id="chartActivity">
        <h2 class="bolder" i18n:translate="">Activities</h2>
        <div id="timeline-chart"></div>
      </div>
      <div id="chartOutput">
        <h2 class="bolder" i18n:translate="">Outputs</h2>
        <div id="timeline-chart"></div>
      </div>
    </div>
    <div class="capacities">
      <div id="chartCapacities" tal:define="outcome view/getOutcomeCC">
        <h2 class="bolder" i18n:translate="">Capacity Changes</h2>
        <div class="legendBox">
          <span class="label custom-legend notset">Empty</span>
          <span class="label custom-legend verybad">-2</span>
          <span class="label custom-legend bad">-1</span>
          <span class="label custom-legend equal">0</span>
          <span class="label custom-legend good">1</span>
          <span class="label custom-legend verygood">2</span>
        </div>
        <div class="information" >
          <h3 class="capacity-title" i18n:translate="">Organitzational transactional dimensions</h3>
          <div class="primerafila">
            <tal:block repeat="item outcome/specifics">
              <tal:block condition="python: item['short_category'] == 'transformational'">
                <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                tal:attributes="
                          data-pk currentYear;
                          data-urloutcomeccs outcome/url;
                          data-id_specific item/id_specific;
                          data-description item/description;
                          data-base-value item/baseline;
                          data-base-date item/baseline_date;
                          data-objective item/objective;
                          data-objective-date item/objective_date;
                          class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                  <div class="item_image" tal:condition="not:item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url}" alt="" title=""/>
                  </div>
                  <div class="item_image " tal:condition="item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                  </div>
                  <div class="item_text">
                    <span tal:content="item/title_specific">Leadership</span>
                  </div>
                </a>
              </tal:block>
            </tal:block>
          </div>
          <h3 class="capacity-title" i18n:translate="">Individual dimensions</h3>
          <div class="segundafila">
            <tal:block repeat="item outcome/specifics">
              <tal:block condition="python: item['short_category'] == 'transactional'">
                <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                tal:attributes="
                          data-pk currentYear;
                          data-urloutcomeccs outcome/url;
                          data-id_specific item/id_specific;
                          data-description item/description;
                          data-base-value item/baseline;
                          data-base-date item/baseline_date;
                          data-objective item/objective;
                          data-objective-date item/objective_date;
                          class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                  <div class="item_image" tal:condition="not:item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url}" alt="" title=""/>
                  </div>
                  <div class="item_image " tal:condition="item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                  </div>
                  <div class="item_text">
                    <span tal:content="item/title_specific">Leadership</span>
                  </div>
                </a>
              </tal:block>
            </tal:block>
          </div>
          <h3 class="capacity-title" i18n:translate="">Organitzational transformational dimensions</h3>
          <div class="tercerafila">
            <tal:block repeat="item outcome/specifics">
              <tal:block condition="python: item['short_category'] == 'individual'">
                <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                tal:attributes="
                          data-pk currentYear;
                          data-urloutcomeccs outcome/url;
                          data-id_specific item/id_specific;
                          data-description item/description;
                          data-base-value item/baseline;
                          data-base-date item/baseline_date;
                          data-objective item/objective;
                          data-objective-date item/objective_date;
                          class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                  <div class="item_image" tal:condition="not:item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url}" alt="" title=""/>
                  </div>
                  <div class="item_image " tal:condition="item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                  </div>
                  <div class="item_text">
                    <span tal:content="item/title_specific">Leadership</span>
                  </div>
                </a>
              </tal:block>
            </tal:block>
          </div>
        </div>
        <div class="information">
          <div class="">
            <tal:block repeat="item outcome/specifics">
              <h3 class="capacity-title" i18n:translate="" tal:condition="python: item['short_category'] == 'other'">Others</h3>
              <tal:block condition="python: item['short_category'] == 'other'">
                <a data-toggle="modal" data-target="#modalEditOutcomeCCS"
                  tal:attributes="
                          data-pk currentYear;
                          data-urloutcomeccs outcome/url;
                          data-id_specific item/id_specific;
                          data-description item/description;
                          data-base-value item/baseline;
                          data-base-date item/baseline_date;
                          data-objective item/objective;
                          data-objective-date item/objective_date;
                          class string:editOutcomeCCS item ${item/short_category} ${item/selected_specific};">
                  <div class="item_image" tal:condition="not:item/selected_specific">
                      <img tal:attributes="src string:${item/icon_url}" alt="" title=""/>
                  </div>
                  <div class="item_image " tal:condition="item/selected_specific">
                    <img tal:attributes="src string:${item/icon_url_selected}" alt="" title=""/>
                  </div>
                  <div class="item_text">
                    <span tal:content="item/title_specific">Other</span>
                  </div>
                </a>
              </tal:block>
            </tal:block>
          </div>
        </div>
      </div>
    </div>

    <script type="text/javascript" src="++theme++gwopa.theme/assets/javascripts/wa-dashboard.js"></script>

  </metal:block>

</metal:content-core>

</body>
</html>
