<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone"
      tal:omit-tag="">


<div class="sidenav2" tal:define="portal_state context/@@plone_portal_state;
                       portal_url portal_state/portal_url;">
    <ul id="sidenav-vertical">
        <a class="plone-btn plone-btn-info active" tal:attributes="href string:${portal_url}" href="#"><i class="fa fa-home" aria-hidden="true"></i> PMP </a>
        <tal:omit-tag tal:condition="not:view/isRootFolder">
            <ul id="sidenav-vertical-overview">
                <a class="plone-btn plone-btn-info" tal:attributes="href string:${portal_url}/areas" href="#"><i class="fas fa-chart-area" aria-hidden="true"></i> Areas </a>
                <a class="plone-btn plone-btn-info" tal:attributes="href string:${portal_url}/teams" href="#"><i class="fas fa-users" aria-hidden="true"></i> Teams </a>
                <a class="plone-btn plone-btn-info" tal:attributes="href string:${portal_url}/files" href="#"><i class="fas fa-file" aria-hidden="true"></i> Files </a>
            </ul>
        </tal:omit-tag>
        <tal:omit-tag tal:condition="view/isRootFolder">
            <ul id="sidenav-vertical-overview">
                <a class="plone-btn plone-btn-info active" tal:attributes="href string:${view/projectPath}" href="#"><i class="fas fa-home" aria-hidden="true"></i> Overview </a>
                <a class="plone-btn plone-btn-info" tal:attributes="href string:${view/projectPath}/projectareas" href="#"><i class="fas fa-chart-area" aria-hidden="true"></i> Areas </a>
                <a class="plone-btn plone-btn-info" tal:attributes="href string:${view/projectPath}/projectteams" href="#"><i class="fas fa-users" aria-hidden="true"></i> Team </a>
                <a class="plone-btn plone-btn-info" tal:attributes="href string:${view/projectPath}/projectfiles" href="#"><i class="fas fa-file" aria-hidden="true"></i> Files </a>
            </ul>
            <a class="plone-btn plone-btn-success" tal:attributes="href string:${view/projectPath}/planning" href="#"><i class="far fa-calendar-check" aria-hidden="true"></i>&nbsp;<tal:omit-tag i18n:translate=""> Planning </tal:omit-tag></a>
            <a class="plone-btn plone-btn-success" tal:attributes="href string:${view/projectPath}/collaborating" href="#"><i class="fa fa-users" aria-hidden="true"></i>&nbsp;<tal:omit-tag i18n:translate=""> Collaboration </tal:omit-tag></a>
            <a class="plone-btn plone-btn-success" tal:attributes="href string:${view/projectPath}/monitoring" href="#"><i class="fas fa-chart-pie" aria-hidden="true"></i>&nbsp;<tal:omit-tag i18n:translate=""> Monitoring </tal:omit-tag></a>
            <a class="plone-btn plone-btn-success" tal:attributes="href string:${view/projectPath}/reporting" href="#"><i class="far fa-file-alt" aria-hidden="true"></i>&nbsp;<tal:omit-tag i18n:translate=""> Reporting </tal:omit-tag></a>
            <a class="plone-btn plone-btn-success" tal:attributes="href string:${view/projectPath}/visualizating" href="#"><i class="fas fa-tachometer-alt" aria-hidden="true"></i>&nbsp;<tal:omit-tag i18n:translate=""> Dashboard </tal:omit-tag></a>
        </tal:omit-tag>
    </ul>
    <ul id="portal-logout">
        <li>
            <a href="" tal:attributes="href string:${portal_url}/logout"><i class="fas fa-power-off"></i>&nbsp;<tal:omit-tag i18n:translate=""> Log out </tal:omit-tag></a>
        </li>
    </ul>
  </div>


</html>
